<AnchorPane prefHeight="771.0" prefWidth="652.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="View.WonPageController">
  <children>
    <Label layoutX="207.0" layoutY="24.0" prefHeight="71.0" prefWidth="239.0" text="You Won!">
      <font>
        <Font size="49.0" />
      </font>
    </Label>
    <ImageView fitHeight="273.0" fitWidth="266.0" layoutX="194.0" layoutY="114.0" pickOnBounds="true" preserveRatio="true">
      <image>
        <Image url="@winner.png" />
      </image>
    </ImageView>
    <Label layoutX="240.0" layoutY="386.0" prefHeight="64.0" prefWidth="173.0" text="Congratulations!">
      <font>
        <Font size="22.0" />
      </font>
    </Label>
    <Label layoutX="253.0" layoutY="443.0" prefHeight="54.0" prefWidth="147.0" text="You did it!">
      <font>
        <Font size="30.0" />
      </font>
    </Label>
    <Button fx:id="playAgainEvent" layoutX="434.0" layoutY="497.0" mnemonicParsing="false" onAction="#pressButtonPlayAgain" prefHeight="64.0" prefWidth="128.0" text="Play Again?">
      <font>
        <Font size="18.0" fx:id="x1" />
      </font>
    </Button>
    <Button fx:id="backMenuEvent" font="$x1" layoutX="58.0" layoutY="497.0" mnemonicParsing="false" onAction="#pressButtonMenu" prefHeight="64.0" prefWidth="195.0" text="Go To Menu" />
  </children>
</AnchorPane>
