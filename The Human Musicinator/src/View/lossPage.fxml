<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="771.0" prefWidth="652.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.lossController">
    <children>
        <Label layoutX="226.0" layoutY="103.0" prefHeight="71.0" prefWidth="239.0" text="You Lost!">
            <font>
                <Font size="49.0" />
            </font>
        </Label>
        <ImageView fitHeight="273.0" fitWidth="266.0" layoutX="193.0" layoutY="215.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@loss.jpg" />
            </image></ImageView>
        <Label layoutX="75.0" layoutY="512.0" prefHeight="64.0" prefWidth="518.0" text="Unfortunately you could not guess the correct name">
            <font>
                <Font size="22.0" />
            </font>
        </Label>
        <Label layoutX="215.0" layoutY="563.0" prefHeight="54.0" prefWidth="239.0" text="maybe next time!">
            <font>
                <Font size="30.0" />
            </font>
        </Label>
        <Button fx:id="playAgainEvent" layoutX="465.0" layoutY="659.0" mnemonicParsing="false" onAction="#pressButtonPlayAgain" prefHeight="64.0" prefWidth="128.0" text="Play Again?">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
        <Button fx:id="backMenuEvent" layoutX="58.0" layoutY="659.0" mnemonicParsing="false" onAction="#pressButtonMenu" prefHeight="64.0" prefWidth="128.0" text="Go To Menu">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
