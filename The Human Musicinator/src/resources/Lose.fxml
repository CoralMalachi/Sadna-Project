<AnchorPane prefHeight="700.0" prefWidth="900.0" style="-fx-background-color: #5394fc" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="controller.LoseController">
  <children>
    <Label layoutX="331.0" layoutY="14.0" prefHeight="71.0" prefWidth="239.0" text="You Lose" textFill="WHITE">
      <font>
        <Font size="49.0" />
      </font>
    </Label>
    <ImageView fitHeight="273.0" fitWidth="266.0" layoutX="296.0" layoutY="133.0" pickOnBounds="true" preserveRatio="true">
      <image>
        <Image url="@Lose.png" />
      </image>
    </ImageView>
    <Button fx:id="playAgainEvent" defaultButton="false" layoutX="517.0" layoutY="491.0" mnemonicParsing="false" onAction="#pressButtonPlayAgain" prefHeight="64.0" prefWidth="128.0" text="Play again">
      <font>
        <Font size="18.0" fx:id="x1" />
      </font>
      <textFill>
        <Color blue="1.000" green="0.400" red="0.000" fx:id="x2" />
      </textFill>
    </Button>
    <Button fx:id="backMenuEvent" defaultButton="false" font="$x1" layoutX="190.0" layoutY="491.0" mnemonicParsing="false" onAction="#pressButtonMenu" prefHeight="64.0" prefWidth="239.0" text="Go back to menu" textFill="$x2" />
